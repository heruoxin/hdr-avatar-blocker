{
  "manifest_version": 3,
  "name": "__MSG_appName__",
  "version": "1.0",
  "description": "__MSG_appDesc__",
  "default_locale": "zh_CN",
  "icons": {
    "128": "assets/icon128.png",
    "256": "assets/icon256.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "assets/icon128.png",
      "256": "assets/icon256.png"
    }
  },
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ]
}